{
  "$schema": "vscode://schemas/color-theme",
  "name": "Midnight Peony",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0a0a",
    "editor.foreground": "#ffffff",
    "editorCursor.foreground": "#ff69b4",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editor.selectionBackground": "#ff69b430",
    "editor.inactiveSelectionBackground": "#ff69b420",
    "editorIndentGuide.background": "#2a2a2a",
    "editorIndentGuide.activeBackground": "#ff69b460",

    "activityBar.background": "#0d0d0d",
    "activityBar.foreground": "#ff69b4",
    "activityBar.activeBorder": "#ff69b4",

    "sideBar.background": "#0d0d0d",
    "sideBar.foreground": "#ffffff",

    "statusBar.background": "#0d0d0d",
    "statusBar.foreground": "#ff69b4",

    "titleBar.activeBackground": "#0a0a0a",
    "titleBar.activeForeground": "#ff69b4",

    "tab.activeBackground": "#121212",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#0a0a0a",
    "tab.inactiveForeground": "#888888",
    "tab.activeBorderTop": "#ff69b4",

    "panel.background": "#101010",
    "terminal.background": "#0a0a0a",
    "terminal.foreground": "#ffffff",
    "terminalCursor.foreground": "#ff69b4",

    "button.background": "#ff69b4",
    "button.foreground": "#000000",
    "button.hoverBackground": "#ff85c1",

    "badge.background": "#ff69b4",
    "badge.foreground": "#000000",

    "dropdown.background": "#1a1a1a",
    "dropdown.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a6a6a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#ff69b4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#f78fb3"
      }
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffaec9"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#ffc0cb"
      }
    },
    {
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#ff94d4"
      }
    },
    {
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff69b4"
      }
    },

    // CSS-specific
    {
      "scope": ["entity.name.tag.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#f78fb3"
      }
    },
    {
      "scope": ["meta.property-list.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["punctuation.terminator.rule.css", "punctuation.separator.key-value.css"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#ff85c1"
      }
    },
    {
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#ffc0cb"
      }
    },
    {
      "scope": ["variable.css", "variable.other.css", "support.variable.css"],
      "settings": {
        "foreground": "#ffaec9"
      }
    },
    {
      "scope": ["variable.function.css", "variable.parameter.css"],
      "settings": {
        "foreground": "#ff94d4"
      }
    },
    {
      "scope": ["keyword.other.unit.css", "support.type.vendored.property-name"],
      "settings": {
        "foreground": "#8888ff"
      }
    },
    {
      "scope": ["entity.pseudo-class.css", "entity.pseudo-element.css"],
      "settings": {
        "foreground": "#ff8ad4"
      }
    },

    {
      "scope": ["entity.name.tag.html", "meta.tag.sgml"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["string.quoted.double.html", "string.quoted.single.html", "meta.attribute.html"],
      "settings": {
        "foreground": "#f78fb3"
      }
    },

    // JavaScript
    {
      "scope": ["variable.other.readwrite", "identifier.js", "meta.definition.variable.js"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["support.function", "entity.name.function.js"],
      "settings": {
        "foreground": "#ffc0cb"
      }
    },
    {
      "scope": ["constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#ff94d4"
      }
    },
    {
      "scope": ["storage.modifier.js", "keyword.control"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },

    // Markdown
    {
      "scope": ["markup.heading", "markup.heading.markdown"],
      "settings": {
        "foreground": "#ff69b4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "foreground": "#ffc0cb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "foreground": "#f78fb3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.raw.code.markdown"],
      "settings": {
        "foreground": "#ffaec9"
      }
    }
    
  ]
}
